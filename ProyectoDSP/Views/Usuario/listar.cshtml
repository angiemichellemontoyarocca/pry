@model IEnumerable<ProyectoDSP.Models.Usuario>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<html>
<head>
</head>
<body>
    <table border="2">
        <tr>
            <td>Id</td>
            <td>Username</td>
            <td>Contraseña</td>
            <td>Paterno</td>
            <td>Materno</td>
            <td>Edad</td>
            <td>Direccion</td>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Username</td>
                <td>@item.Contraseña</td>
                <td>@item.Paterno</td>
                <td>@item.Materno</td>
                <td>@item.Edad</td>
                <td>@item.Direccion</td>
            </tr>
        }
    </table>
</body>
</html>